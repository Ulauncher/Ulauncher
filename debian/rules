#!/usr/bin/make -f
export DH_VERBOSE = 1


%:
ifneq ($(shell dh -l | grep -xF translations),)
	dh $@ --with python3,translations --buildsystem=pybuild
else
	dh $@ --with python3 --buildsystem=pybuild
endif


override_dh_auto_configure:
	cd data/preferences && yarn install && yarn build
	dh_auto_configure
